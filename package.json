  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": ["eslint --fix", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Copyes/rollup-library-seed.git"
  },
  "keywords": ["rollup", "library"],
  "author": "Charming",
  "license": "MIT",
  "bugs": {
