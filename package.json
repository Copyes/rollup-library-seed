{
  "name": "rollup-library-seed",
  "version": "1.0.0",
  "description": "js library seed",
  "jsnext:main": "dist/rollup-library-seed.esm.js",
  "main": "dist/rollup-library-seed.js",
  "module": "dist/rollup-library-seed.esm.js",
  "unpkg": "dist/rollup-library-seed.min.js",
  "scripts": {
    "cz": "git-cz",
    "clean": "rimraf ./dist",
    "lint": "eslint -c src/**/*.js --fix",
    "serve": "serve -p 9090",
    "dev": "yarn run clean && cross-env NODE_ENV=development rollup -w -c ./builds/rollup.config.dev.js",
    "build": "yarn run clean && cross-env NODE_ENV=production rollup -c ./builds/rollup.config.prod.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run lint && nyc mocha",
    "release": "npn test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Copyes/rollup-library-seed.git"
  },
  "keywords": [
    "rollup",
    "library",
    "starter",
    "babel"
  ],
  "author": "Fan Chao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Copyes/rollup-library-seed/issues"
  },
  "homepage": "https://github.com/Copyes/rollup-library-seed#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-istanbul": "^5.1.0",
    "commitizen": "^2.9.6",
    "commitlint-config-cz": "^0.8.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.5",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "expect": "^23.6.0",
    "husky": "^1.0.0-rc.6",
    "lint-staged": "^7.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.1.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "serve": "^6.5.7",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0"
  }
}