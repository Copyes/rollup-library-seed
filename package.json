{
  "name": "rollup-library-seed",
  "version": "1.0.0",
  "description": "",
  "main": "dist/rollup-library-seed.cjs.js",
  "module": "dist/rollup-library-seed.esm.js",
  "unpkg": "dist/rollup-library-seed.min.js",
  "scripts": {
    "cz": "git-cz",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.js --fix",
    "serve": "serve -p 9090",
    "dev": "yarn run clean && cross-env NODE_ENV=development rollup -w -c ./builds/rollup.config.dev.js",
    "build": "yarn run clean && cross-env NODE_ENV=production rollup -c ./builds/rollup.config.prod.js",
    "test": "npm run flow && jest --coverage",
    "flow": "flow check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Copyes/rollup-library-seed.git"
  },
  "keywords": [
    "rollup",
    "library",
    "starter",
    "babel"
  ],
  "author": "Charming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Copyes/rollup-library-seed/issues"
  },
  "homepage": "https://github.com/Copyes/rollup-library-seed#readme",
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^2.9.6",
    "commitlint-config-cz": "^0.8.0",
    "cross-env": "^5.1.5",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "flow-bin": "^0.72.0",
    "husky": "^1.0.0-rc.6",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "serve": "^6.5.7",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
